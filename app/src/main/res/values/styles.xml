<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#21EDC4</item>
        <item name="colorPrimaryDark">#09CCB3</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="backgroundColor">#B1B9BA</item>

    </style>
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" >
        <item name="colorPrimary">#41786D</item>
        <item name="colorPrimaryDark">#1D383C</item>
        <item name="colorAccent">#E811B87D</item>

    </style>
    <style name="ShapeAppearanceOverlay.App.CornerSize50Percent" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
        <item name="backgroundTint">@android:color/white</item>        </style>
</resources>